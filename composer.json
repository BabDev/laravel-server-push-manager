{
    "name": "babdev/laravel-server-push-manager",
    "license": "MIT",
    "description": "HTTP/2 server push manager for Laravel",
    "keywords": ["laravel", "server-push"],
    "require": {
        "php": "^7.1.3",
        "fig/link-util": "~1.0",
        "illuminate/support": "5.7.*|5.8.*",
        "psr/link": "~1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14.2",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\ServerPushManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabDev\\ServerPushManager\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BabDev\\ServerPushManager\\ServerPushManagerServiceProvider"
            ]
        }
    }
}
